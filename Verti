precision=rep(0,times=19),cobertura=rep(0,times=19),F1=rep(0,times=19))\n","  cont <- 1\n","  for (cada in seq(0.05,0.95,by = 0.05)){\n","    datos<-metricas(confusion(real,scoring,cada))\n","    registro<-c(cada,datos)\n","    umbrales[cont,]<-registro\n","    cont <- cont + 1\n","  }\n","  return(umbrales)\n","}\n","#Funciones que calculan la curva ROC y el AUC\n","\n","roc<-function(prediction){\n","  r<-performance(prediction,'tpr','fpr')\n","  plot(r)\n","}\n","\n","auc<-function(prediction) {\n","  a<-performance(prediction,'auc')\n","  return(a@y.values[[1]])\n","}"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"r3a2e0U9wKuA"},"source":["#Establecemos una semilla para obtener los mismos resultados\n","set.seed(12345)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"EKpYOyeNfjBc"},"source":["#Dividimos conjunto de train 70%  y test 30% \n","split = sample.split(df$target, SplitRatio = 0.70)\n","train = subset(df, split == TRUE)\n","test = subset(df, split == FALSE)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"0UOqZ0Y-zlcL"},"source":["glimpse(train)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"Mn-z0MJMqRdK"},"source":["#Para el conjunto de entrenamiento eligiremos todas las variables menos la target y las asignaremos a una variable llamada independientes\n","independientes <- setdiff(names(df),c('Customer','target'))\n","target <- 'target'"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"FTDqUf-OxE7w"},"source":["#Creamos una fórmula para usar en el modelo\n","formula <- reformulate(independientes,target)"],"execution_count":null,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"Txg7sW30wlpy"},"source":["**Regresión Logística**"]},{"cell_type":"code","metadata":{"id":"i8w0ofERxMUu"},"source":["#Modelamos con Regresión Logística\n","\n","formula_rl <- formula\n","rl<- glm(formula_rl,train,family=binomial(link='logit'))\n","summary(rl)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":35},"id":"G-eZGWAeyA3j","executionInfo":{"status":"ok","timestamp":1616577609277,"user_tz":-60,"elapsed":804,"user":{"displayName":"Jorge Arias","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GiEYmcbtboNiqsqX3bUNGDSaj0xKA6KeOSewKWPfw=s64","userId":"07140929714598146420"}},"outputId":"febd9768-c347-4a27-9388-5483729db67b"},"source":["#Calculamos R2\n","pr2_rl <- 1 -(rl$deviance / rl$null.deviance)\n","pr2_rl"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["[1] 1"],"text/latex":"0.999999999954335","text/markdown":"0.999999999954335","text/html":["0.999999999954335"]},"metadata":{"tags":[]}}]},{"cell_type":"code","metadata":{"id":"ql9TuQILy4-a"},"source":["#Ahora aplicamos el modelo en el conjunto de test\n","rl_predict<-predict(rl,test,type = 'response')"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":437},"id":"Aget2-zBzEMi","executionInfo":{"status":"ok","timestamp":1616498486357,"user_tz":-60,"elapsed":736,"user":{"displayName":"Jorge Arias","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GiEYmcbtboNiqsqX3bUNGDSaj0xKA6KeOSewKWPfw=s64","userId":"07140929714598146420"}},"outputId":"af26010d-a915-40e7-9359-b5c21986881a"},"source":["plot(rl_predict~test$target)"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"image/png":"iVBORw0KGgoAAAANSUhEUgAAA0gAAANICAMAAADKOT/pAAAC6FBMVEUAAAABAQECAgIDAwME\nBAQFBQUGBgYHBwcICAgJCQkKCgoLCwsMDAwNDQ0ODg4PDw8QEBARERESEhITExMUFBQVFRUW\nFhYXFxcYGBgZGRkaGhobGxscHBwdHR0eHh4fHx8gICAhISEiIiIjIyMkJCQlJSUmJiYnJyco\nKCgpKSkqKiosLCwtLS0uLi4vLy8wMDAxMTEyMjIzMzM0NDQ1NTU2NjY3Nzc4ODg5OTk6Ojo7\nOzs8PDw9PT0+Pj4/Pz9AQEBBQUFCQkJDQ0NERERFRUVGRkZHR0dISEhJSUlKSkpLS0tMTExN\nTU1OTk5PT09QUFBRUVFSUlJTU1NUVFRVVVVWVlZXV1dYWFhZWVlaWlpbW1tcXFxdXV1eXl5f\nX19gYGBh